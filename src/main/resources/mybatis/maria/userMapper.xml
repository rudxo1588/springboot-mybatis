<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.demo.user.mapper">

	<insert id="insert" parameterType="com.mybatis.demo.user.vo.User">
		INSERT INTO USER
		(
			 id
			,password	
			,name
			,email
			,phone
		)
		VALUES
		(
			 #{id}
			,#{password}
			,#{name}
			,#{email}
			,#{phone}
		)
	</insert>

</mapper>